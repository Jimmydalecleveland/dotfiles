  },
  "remote.extensionKind": {
    "atlassian.atlascode": "workspace"
  },
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [
        "\"",
        "0",
        "p"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        " ",
        "e"
      ],
      "commands": [
        {
          "command": "editor.action.marker.nextInFiles"
        }
      ]
    },
    {
      "before": [
        " ",
        "E"
      ],
      "commands": [
        {
          "command": "editor.action.marker.prevInFiles"
        }
      ]
    },
    {
      "before": [
        "tab"
      ],
      "commands": [
        {
          "command": "workbench.action.navigateEditorGroups"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [
        "\"",
        "0",
        "p"
      ]
    },
    {
      "before": [
        "u"
      ],
      "after": [],
      "commands": [
        {
          "command": "undo",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "after": [],
      "commands": [
        {
          "command": "redo",
          "args": []
        }
      ]
    },
    {
      "before": [
        " ",
        "w"
      ],
      "after": [
        "leader",
        "leader",
        "leader",
        "b",
        "d",
        "w"
      ]
    },
    {
      "before": [
        " ",
        "j",
        "k"
      ],
      "after": [
        "leader",
        "leader",
        "leader",
        "b",
        "d",
        "j",
        "k"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerYOffset": 6,
  "vim.easymotionMarkerWidthPerChar": 9,
  "vim.easymotionMarkerFontWeight": "bold",
  "remote.SSH.configFile": "C:\\Users\\jimmy\\.ssh\\config",
  "workbench.iconTheme": "material-icon-theme",
  "atlascode.jira.workingProject": {
    "id": "15147",
